package February19;

import java.util.Scanner;


public class GUESSRT {
	class Fraction{
		int num;
		int den;
		Fraction(int num , int den){
			this.num = num;
			this.den = den;
		}
		public void reduce() {
			int gcd = GUESSRT.gcd(this.num, this.den);
			System.out.println(gcd);
			
		}
	}
	
	public static int gcd(int a , int b) {
		if(a<b) {
			return gcd(b,a);
		}
		if(b==0) {
			return a;
		}
		return gcd(b,a%b);
	}
	
