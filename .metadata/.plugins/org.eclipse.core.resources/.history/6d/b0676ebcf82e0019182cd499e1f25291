import java.util.Scanner;

public class Penalty {

	public static void main(String[] args) {
		Scanner s = new Scanner( System.in);
		String ind = s.next();
		String pak = s.next();
		char[] arr1 = ind.toCharArray();
		char[] arr2 = pak.toCharArray();
		int count1=0;
		int count2=0;
		for(int i=0;i<arr1.length;i++) {
			if(arr1[i]=='1') {
				count1++;
			}
		}

	}

}
